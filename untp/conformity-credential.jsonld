{
    "@context": {
      "@protected": true,
  
      "@vocab": "https://www.w3.org/ns/credentials/issuer-dependent#",
  
      "untp": "https://test.uncefact.org/vocabulary/untp/cc/",
  
      "DigitalConformityCredential": {
        "@id": "untp",
        "@context": {
          "@protected": true
        }
      },
  
      "ConformityAttestation": {
        "@id": "untp:ConformityCredential#conformityattestation",
        "@context": {
          "@protected": true,
  
          "scope": "untp:ConformityAssessmentScheme",
          "issuedBy": "untp:issuedBy",
          "issuedTo": "untp:issuedTo",
          "assessments": "untp:conformityassessment",
          "regulatoryApproval": "untp:regulatoryApproval",
          "conformityCertificate": "untp:conformityCertificate"
        }
      },
  
      "ConformityAssessment": {
        "@id": "untp:ConformityCredential#conformityassessment",
        "@context": {
          "@protected": true,
  
          "untp": "https://uncefact.github.io/spec-untp/docs/specification/",
  
          "referenceStandard": "untp:sustainabilitytopic",
          "referenceRegulation": "untp:referenceRegulation",
          "assessmentCriterion": "untp:assessmentCriterion",
          "subjectProducts": "untp:subjectProducts", 
          "subjectFacilities": "untp:subjectFacilities",
          "measuredResults": "untp:measuredResults",
          "compliance": "untp:compliance",
          "sustainabilityTopic": "untp:sustainabilityTopic"
        }
      },
  
      "ConformityEvidence": {
        "@id": "untp:ConformityCredential#conformityattestation",
        "@context": {
          "@protected": true,
  
          "evidenceRootHash": "untp:evidenceRootHash",
          "evidenceData": "untp:evidenceData",
          "encryptionMethod": "untp:encryptionMethod",
          "decryptionKeyRequest": "untp:decryptionKeyRequest"
        }
      },
  
      "Authority": {
        "@id": "untp:ConformityCredential#conformityattestation",
        "@context": {
          "@protected": true,
  
          "number": "untp:number",
          "accreditationCertificate": "untp:accreditationCertificate",
          "trustmark": "untp:trustmark",
          "authority": "untp:authority"
        }
      },
  
      "Standard": {
        "@id": "untp:ConformityCredential#conformityattestation",
        "@context": {
          "@protected": true,
  
          "issuingBody": "untp:issuingBody",
          "issuanceDate": "untp:issuanceDate"
        }
      },
  
      "Regulation": {
        "@id": "untp:ConformityCredential#conformityattestation",
        "@context": {
          "@protected": true,
  
          "issuingBody": "untp:issuingBody",
          "effectiveDate": "untp:effectiveDate"
        }
      },
  
      "Criteria": {
        "@id": "untp:ConformityCredential#conformityattestation",
        "@context": {
          "@protected": true,
  
          "threshold": "untp:issuingBody"
        }
      }
    }
  }
